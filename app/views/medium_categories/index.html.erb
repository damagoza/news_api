<p id="notice"><%= notice %></p>

<h1>Listing Medium Categories</h1>

<table>
  <thead>
    <tr>
      <th>Medium</th>
      <th>Category</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medium_categories.each do |medium_category| %>
      <tr>
        <td><%= medium_category.medium %></td>
        <td><%= medium_category.category %></td>
        <td><%= medium_category.url %></td>
        <td><%= link_to 'Show', medium_category %></td>
        <td><%= link_to 'Edit', edit_medium_category_path(medium_category) %></td>
        <td><%= link_to 'Destroy', medium_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medium category', new_medium_category_path %>
