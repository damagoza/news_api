<p id="notice"><%= notice %></p>

<h1>Listing News Extracts</h1>

<table>
  <thead>
    <tr>
      <th>Mediumcategory</th>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @news_extracts.each do |news_extract| %>
      <tr>
        <td><%= news_extract.MediumCategory %></td>
        <td><%= news_extract.title %></td>
        <td><%= news_extract.content %></td>
        <td><%= link_to 'Show', news_extract %></td>
        <td><%= link_to 'Edit', edit_news_extract_path(news_extract) %></td>
        <td><%= link_to 'Destroy', news_extract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New News extract', new_news_extract_path %>
